<html>
    <%- include('./partials/headscript.ejs') %>

  <body style="margin : 0px; overflow: hidden;">
    <h1><%= project.filePath %></h1>

        <!-- arjs market 3d model  -->

    <a-scene embedded arjs>

      <a-marker preset="hiro">
        <a-entity
          position="0 0 0"
          scale="1 1 1"
          gltf-model="<%= project.filePath %>"
        ></a-entity>
      </a-marker>
      <a-entity camera></a-entity>

    </a-scene>


  </body>
  
</html>

